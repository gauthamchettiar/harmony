[{"source": "readme", "content": [{"Parameter": "host", "type": "string", "required": "yes", "default": "-", "description": "mysql/mariadb host"}, {"Parameter": "port", "type": "int", "required": "yes", "default": "-", "description": "mysql/mariadb port"}, {"Parameter": "user", "type": "string", "required": "yes", "default": "-", "description": "db username"}, {"Parameter": "password", "type": "string", "required": "yes", "default": "-", "description": "db password"}, {"Parameter": "cursorclass", "type": "string", "required": "No", "default": "pymysql.cursors.SSCursor", "description": "set cursorclass used by PyMYSQL"}, {"Parameter": "database", "type": "string", "required": "No", "default": "-", "description": "Database to use, None to not use a particular one. Used by PyMYSQL"}, {"Parameter": "server_id", "type": "int", "required": "False", "default": "Randomly generated int", "description": "Used as the slave id when this tap is connecting to the server"}, {"Parameter": "filter_dbs", "type": "string", "required": "False", "default": "-", "description": "Comma separated list of schemas to extract tables only from particular schemas and to improve data extraction performance"}, {"Parameter": "use_gtid", "type": "bool", "required": "False", "default": "False", "description": "Flag to enable log based replication using GTID"}, {"Parameter": "engine", "type": "string ('mysql' or 'mariadb')", "required": "False", "default": "'mysql'", "description": "Indicate which flavor the server is, used for LOG_BASED with GTID"}, {"Parameter": "ssl", "type": "string (\"true\")", "required": "No", "default": "False", "description": "Enable SSL connection"}, {"Parameter": "ssl_ca", "type": "string", "required": "No", "default": "-", "description": "for self-signed SSL"}, {"Parameter": "ssl_cert", "type": "string", "required": "No", "default": "-", "description": "for self-signed SSL"}, {"Parameter": "ssl_key", "type": "string", "required": "No", "default": "-", "description": "for self-signed SSL"}, {"Parameter": "internal_hostname", "type": "string", "required": "No", "default": "-", "description": "Override match hostname for google cloud"}, {"Parameter": "session_sqls", "type": "List of strings", "required": "No", "default": "['SET @@session.time_zone=\"+0:00\"', 'SET @@session.wait_timeout=28800', 'SET @@session.net_read_timeout=3600', 'SET @@session.innodb_lock_wait_timeout=3600']", "description": "Set session variables dynamically."}], "instance": 0}]